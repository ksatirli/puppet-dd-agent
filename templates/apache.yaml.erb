init_config:

instances:
    -   apache_status_url: <%= @apache_status_url %>
        <% unless @apache_user == nil %>apache_user: <%= @apache_user %><% end %>
        <% unless @apache_password == nil %>apache_password: <%= @apache_password %><% end %>
        <% if @tags.kind_of?(Array) %>tags:
            -	<% @tags.each do |tag| %><%= tag %> <% end end %>